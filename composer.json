{
    "name": "jorpo/specification",
    "description": "Simple specification pattern implementation.",
    "type": "library",
    "license": "GNU GPLv3",
    "authors": [
        {
            "name": "John Porter",
            "email": "john@designermonkey.co.uk"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Jorpo\\Specification\\": [
                "src"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "vimeo/psalm": "^4.10"
    },
    "autoload-dev": {
        "psr-4": {
            "Jorpo\\Specification\\": [
                "tests"
            ]
        }
    }
}
